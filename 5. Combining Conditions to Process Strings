# Example: Check strings for specific conditions
words = ["apple", "banana", "cherry", "date", "elderberry"]
for word in words:
    if word[0].lower() in "aeiou" and "e" in word:
        print(f"'{word}' starts with a vowel and contains 'e'")
    elif "e" in word:
        print(f"'{word}' contains 'e' but does not start with a vowel")
    else:
        print(f"'{word}' does not meet any condition")
